#!/usr/bin/env bash

set -eux

srcdir=@CMAKE_CURRENT_SOURCE_DIR@
bindir=@CMAKE_CURRENT_BINARY_DIR@
datadir=@CMAKE_SOURCE_DIR@/tests/data
exedir=@CMAKE_BINARY_DIR@/bin


eng_type=tflite

model_path=$datadir/model.tflite
input_path=$datadir/m36966_input.npy
ref_pred=$datadir/ref_pred.csv

output_path=$bindir/prediction_tflite.csv


# run inference
$exedir/infero.x \
--model=$model_path \
--input=$input_path \
--engine=$eng_type \
--output=$output_path \
--ref_path=$ref_pred


